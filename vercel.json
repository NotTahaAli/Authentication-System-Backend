{
    "version": 2,
    "buildCommand": "npm run migrate",
    "installCommand": "npm install",
    "builds": [
        {
        "src": "dist/index.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}